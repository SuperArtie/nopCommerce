@using Newtonsoft.Json
@using Nop.Web.Framework.TagHelpers.Admin
@model int

@{
    var items = (List<SelectListItem>)ViewData["SelectList"];
    var itemJson = JsonConvert.SerializeObject(items.Select(x => new { x.Text, x.Value }));
    var startIndex = items.Count > 1 ? items.FirstOrDefault(x => x.Value == Model.ToString()) != null ? items.IndexOf(items.FirstOrDefault(x => x.Value == Model.ToString())) : 0 : 0;
    var url = ViewData.Keys.Contains("SearchAction") ? ViewData["SearchAction"].ToString() : "/Admin/SearchSelect/DefaultSelectSearch";
    var propName = ViewData.Keys.Contains("SearchAction") ? string.Empty : Html.NameForModel();
}

<input id="@Html.IdForModel()" name="@Html.NameForModel()" />

<script>
    $(document).ready(function() {

        var searchableSelect = new SearchableSelect("#@Html.IdForModel()", "@url", "@propName");
        searchableSelect.init(@Html.Raw(itemJson), @startIndex, "@items.First().Value", "@items.First().Text");
    });
</script>